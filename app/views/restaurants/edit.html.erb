<h1>Edit <%= @restaurant.name %> </h1>
<%= simple_form_for @restaurant do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :category %>
  <%= f.button :submit %>
<% end %>

<%= link_to "return", restaurants_path %>
<%= link_to "destroy", restaurant_path(@restaurant), method: :delete %>

